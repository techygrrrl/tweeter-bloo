{
  "manifest_version": 3,
  "name": "Tweeter Bloo",
  "version": "0.0.0",
  "description": "Inserts blue poo emojis next to Twitter users who subscribe to Twitter Blue",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "author": "techygrrrl",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ]
}